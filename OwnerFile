local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
	Name = "                              EXPI-HUB",
	LoadingTitle = "WELCOME TO EXPI HUB",
	LoadingSubtitle = "MADE BY Caleb*#7450",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Expi Hub"
	},
        Discord = {
        	Enabled = true,
        	Invite = "Unknown", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "Expi-Hub Key System",
		Subtitle = "Key System",
		Note = "Test Gui!",
		FileName = "KEY SYSTEM",
		SaveKey = false,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "test"
	}
})


Rayfield:Notify({
    Title = "Made by Caleb*#7540",
    Content = "All Script's Made By: Caleb*7540",
    Duration = 6.5,
    Image = 200081952,
    Actions = { -- Notification Buttons
        Ignore = {
            Name = "Ate!",
            Callback = function()
                print("The user tapped Yes!")
            end
		},
	},
})








