local Main = Tab:CreateButton({
	Name = "Emmas Keybind (B)",
	Callback = function()
        print("Error")
        warn("WARNED")
        wait(0.1)
        warn("WARNED")
        print("Running Notifaction")
        warn("Banned")
        warn("Notifaction pop!")
        wait(0.1)
        warn("Exlcuding Kick")
        -- game.Players.H00chi:Kick("Client Dissconected")
        print("Yes Or No")
        print("Yes Selected!")
        wait(1)
        warn("UI FATAL ERROR")
        Rayfield:Notify({
            Title = "Expi-Hub",
            Content = "FATAL ERROR",
            Duration = 4,
            Image = 4483362458,
            Actions = { -- Notification Buttons
               Ignore = {
                  Name = "Okay",
                  Callback = function()
                  print("The user tapped Okay!")
               end
            },
         },
         })
    end,
})



